{
  "manifest_version": 2,
  "version": "1.1.1",
  "name": "Shopify Pixel Trigger",
  "description": "Trigger for facebook pixel on shopify store.",
  "author": "Adenilson Santos",
  "browser_action": {
    "default_title": "Shopify Pixel Trigger",
    "default_popup": "index.html",
    "default_icon": "flame.png"
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "web_accessible_resources": [
    "script.js"
  ]
}